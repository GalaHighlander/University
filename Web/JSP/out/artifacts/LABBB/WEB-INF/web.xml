<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.login.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.login.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.login.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CityProcessor</servlet-name>
        <servlet-class>com.login.CityProcessor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CityProcessor</servlet-name>
        <url-pattern>/CityProcessor</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CityAdder</servlet-name>
        <servlet-class>com.login.CityAdder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CityAdder</servlet-name>
        <url-pattern>/CityAdder</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Stop</servlet-name>
        <servlet-class>com.login.service.Stop</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Stop</servlet-name>
        <url-pattern>/Stop</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Next</servlet-name>
        <servlet-class>com.login.Next</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Next</servlet-name>
        <url-pattern>/Next</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Back_Current</servlet-name>
        <servlet-class>com.login.Back_Current</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Back_Current</servlet-name>
        <url-pattern>/Back_Current</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Back_Neighbour</servlet-name>
        <servlet-class>com.login.Back_Neighbour</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Back_Neighbour</servlet-name>
        <url-pattern>/Back_Neighbour</url-pattern>
    </servlet-mapping>

</web-app>